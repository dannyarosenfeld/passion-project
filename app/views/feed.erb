<h1>Your Feed</h1>
<h2>Check out logs of people you're following</h2>


  <div id="map"></div>

<% @current_user.followees.each do |followee| %>
  <%followee.logs.each do |log|%>
      <img src="<%= log.user.image %>" alt="user image">
      <p class="paragraph"> <%= log.location %> </p>
  <p> <%= log.likes.count %> people favorited this workout log</p>
   <a href="/logs/<%=log.id %>"><%= log.title %></a>
  <p> <%= log.duration %> </p>
  <% end %>
<% end %>

<meta name="viewport" content="initial-scale=1.0">

<style>
  html, body {
    height: 100%;
    margin: 0;
    padding: 0;
  }
  #map {
    height: 60%;
    width:70%;
    float: right;
    border-radius: 10px;
    border: 3.4px solid #47B28C;
  }
 #floating-panel {
  color: black;
  margin-top: 100px;
        position: absolute;
        top: 10px;
        left: 25%;
        z-index: 5;
        background-color: #fff;
        padding: 5px;
        border: 1px solid #999;
        text-align: center;
        font-family: 'Roboto','sans-serif';
        line-height: 30px;
        padding-left: 10px;
      }
    </style>
  <script src="js/feedmarkers.js"></script>

 <script src="https://maps.googleapis.com/maps/api/js?key=&callback=initMap"
async defer></script>
