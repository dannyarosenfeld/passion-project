  <div class="pushdown"></div>

    <h1>Your Feed</h1>
    <h2>Check out logs of people you're following</h2>

    <div style="height:100%; width:100%; display:inline; margin-bottom: -300px;">
           <div id="map"></div>
    </div>

<div class="containerblock">
<div class="containerthing">


<% @current_user.followees.each do |followee| %>
  <%followee.logs.each do |log|%>
      <img src="<%= log.user.image %>" alt="user image">
      <p class="userinfo"><%= log.user.username %></p>
      <p class="paragraph"> <%= log.location %> </p>
      <p class="created"><%= log.created_at.strftime("%d %b. %Y") %></p>

  <p> <%= log.likes.count %> people favorited this workout log</p>
   <a href="/logs/<%=log.id %>"><%= log.title %></a>
  <p> <%= log.duration %> </p>
  <% end %>
<% end %>
</div>
</div>
